<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="~/twitter-bootstrap/css/bootstrap.min.css" />
    <script src="~/twitter-bootstrap/js/bootstrap.bundle.min.js"></script>
    <script>
        $(function () {
            $('[data-toggle="tooltip"]').tooltip();
        })
    </script>
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark mb-3">
        <a class="navbar-brand" asp-controller="Home" asp-action="Index">
            <span class="text-white">COVID-19 DATA</span>
        </a>
        <ul class="navbar-nav mr-auto ml-5">
            <li class="nav-item">
                <a class="nav-link" asp-controller="Home" asp-action="Index">Home</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" asp-controller="National" asp-action="Current">Current</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" asp-controller="National" asp-action="Historic">Historic</a>
            </li>
            <li>
                <a class="nav-link" asp-controller="States" asp-action="Current">States</a>
            </li>
        </ul>
    </nav>
    <div class="container">
        @RenderBody()
    </div>
    <script src="~/jquery/jquery.slim.min.js"></script>
    <script src="~/twitter-bootstrap/js/bootstrap.bundle.min.js"></script>
    <script>
        $(function ($) {
            $('[data-toggle="tooltip"]').tooltip();
        })(jQuery)
    </script>
</body>
</html>
